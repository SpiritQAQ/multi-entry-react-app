{
    "presets": ["react-app"],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ],
        [
            "import",
            {
                "libraryName": "antd-mobile",
                "style": true
            },
            "import-antd"
        ]
    ]
}
